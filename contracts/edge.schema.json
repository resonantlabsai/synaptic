{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Synaptic Edge (neighbor/coact)",
  "type": "object",
  "required": [
    "src",
    "dst",
    "kind",
    "weight",
    "n",
    "last_ts"
  ],
  "properties": {
    "src": {
      "type": "string"
    },
    "dst": {
      "type": "string"
    },
    "kind": {
      "type": "string",
      "enum": [
        "neighbor",
        "coact"
      ]
    },
    "weight": {
      "type": "number"
    },
    "n": {
      "type": "integer"
    },
    "last_ts": {
      "type": "string"
    }
  }
}