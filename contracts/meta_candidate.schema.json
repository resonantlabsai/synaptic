{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Synaptic Meta Candidate",
  "type": "object",
  "required": [
    "title",
    "summary",
    "members",
    "score"
  ],
  "properties": {
    "title": {
      "type": "string"
    },
    "summary": {
      "type": "string"
    },
    "members": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "score": {
      "type": "number"
    },
    "reasons": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}